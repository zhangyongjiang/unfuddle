function sms(phone, body) {
	var extra = {
		"address" : phone,
		"sms_body" : body
	};
	var json = JSON.stringify(extra);
	Device.startActivity(Intent.ACTION_VIEW, "smsto:" + phone, "vnd.android-dir/mms-sms", null, json);
}

