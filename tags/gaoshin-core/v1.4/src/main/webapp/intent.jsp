//startActivity(String action, String uri, String type, String data, String extra) {
function browse(url) {
	Device.startActivity(Intent.ACTION_VIEW, url, null, null, null);
}

function map(lat, lng) {
	Device.startActivity(Intent.ACTION_VIEW, "geo:" + lat + "," + lng, null, null, null);
}

function dial(phone) {
	Device.startActivity(Intent.ACTION_DIAL, "tel:" + phone, null, null, null);
}

function call(phone) {
	Device.startActivity(Intent.ACTION_CALL, "tel:" + phone, null, null, null);
}

